<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="UserRepository suit">
    <test name="Interaction with UserRepository Suit" preserve-order="true" verbose="2">
        <groups>
            <run>
                <include name="HAS_INTERACTION_WITH_USER_REPOSITORY"/>
            </run>
        </groups>
        <packages>
            <package name="login.*"/>
        </packages>
    </test>
</suite>